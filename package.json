{
  "name": "finance-expense-nextjs-supabase",
  "version": "0.0.1",
  "private": true,
  "description": "next application",
  "repository": {
    "type": "git",
    "url": "https://github.com/elartix/finance-expense-nextjs-supabase"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "dev": "next dev",
    "dev:local": "env-cmd -f .env.local -x next dev --hostname=$HOSTNAME --port=$PORT",
    "build": "next build",
    "build:local": "env-cmd -f .env.local -x next build",
    "build:development": "NODE_ENV=development env-cmd -f .env.development -x next build",
    "start": "next start",
    "start:development": "env-cmd -f .env.development -x next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .cache build coverage .next analyze",
    "postbuild": "next-sitemap && cpy 'models/**/*.swagger.yaml' public/openapi --flat",
    "openapi:yaml": "npx typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'src/models/openapi' 'models/*.ts'",
    "openapi:swagger": "next-swagger-doc-cli next-swagger-doc.json"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@headlessui/tailwindcss": "^0.1.2",
    "@heroicons/react": "^2.0.17",
    "@next/bundle-analyzer": "^13.3.0",
    "@next/font": "^13.3.0",
    "@redux-devtools/extension": "^3.2.5",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "next": "^13.3.0",
    "next-compose-plugins": "^2.2.1",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.0.0",
    "next-sitemap": "^4.0.7",
    "next-swagger-doc": "^0.3.6",
    "postcss-discard-comments": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^7.2.9",
    "react-use": "^17.4.0",
    "react-vh": "^1.1.1",
    "redux": "^4.2.1",
    "redux-form": "^8.3.9",
    "redux-saga": "^1.2.3",
    "redux-saga-controller": "^1.2.0",
    "swagger-ui-react": "^4.18.2",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/redux-form": "^8.3.5",
    "@types/swagger-ui-react": "^4.18.0",
    "@types/yup": "0.29.14",
    "autoprefixer": "^10.4.14",
    "env-cmd": "^10.1.0",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.21",
    "postcss-calc": "^8.2.4",
    "postcss-discard-duplicates": "^6.0.0",
    "postcss-discard-empty": "^6.0.0",
    "postcss-env-function": "^5.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-normalize-charset": "^6.0.0",
    "postcss-normalize-display-values": "^6.0.0",
    "postcss-normalize-positions": "^6.0.0",
    "postcss-ordered-values": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "postcss-svgo": "^6.0.0",
    "postcss-unique-selectors": "^6.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.1",
    "tailwindcss": "^3.3.1",
    "typeconv": "2.3.0",
    "typescript": "4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test",
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
